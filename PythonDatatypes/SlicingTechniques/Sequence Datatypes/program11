#take arr1/list1, arr2/list2 with some random numbers. take an empty arr3/list3 of relevant size. 
#copy first arr1/list1 into arr3/list3. copy the next arr2/list2 elements into arr3/list3
import random
list1 = [random.randint(1, 50) for _ in range(5)]
list2 = [random.randint(51, 100) for _ in range(5)]

print("List 1:", list1)
print("List 2:", list2)
list3 = []
for num in list1:
    list3.append(num)
for num in list2:
    list3.append(num)
print("Merged List (list3):", list3)
